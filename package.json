  "scripts": {
    "build-dev": "webpack --watch",
    "build": "webpack",
    "start-dev": "nodemon build/server.js",
    "start": "node build/server.js",
    "test": "jest --runInBand --config .jest/config.json",
    "start:worker": "npm run start -- --service-worker",
    "build:prod": "webpack -p",
    "build:dev": "webpack -d",
    "watch": "webpack -d --watch",
    "storybook": "start-storybook -p 9001 -c src/storybook/ -s src/",
    "@storybook": "start-storybook -p 9002",
    "codegen": "apollo codegen:generate --target=typescript types",
    "codegen-watch": "npm run codegen -- --watch",
    "tslint": "tslint 'src/**/*.ts?(x)'",
    "tsc": "npx tsc --noEmit & npx tsc --noEmit -p src/@sdk & npx tsc --noEmit -p src/@next",
    "heroku-postbuild": "npm run build",
    "sitemap": "ts-node ./src/sitemap-builder.ts",
    "generate": "npx plop --plopfile ./.plop/index.js",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "start-server": "pm2 start ./src/server.js -i 0 --name storefront",
    "stop-server": "pm2 stop storefront",
    "serve:prod": "./start-server-prod.sh",
    "serve:dev": "./start-server-dev.sh",
    "serve:devbuild": "./start-server-dev-build.sh",
    "serve:prodbuild": "./start-server-prod-build.sh",
    "copy": "cp ./src/static/* ./dist/"
